<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <pipelines>
      <initialize>
        <processor type="Sitecore.Pipelines.Loader.ShowVersion, Sitecore.Kernel">
          <assemblies>
            <assembly refId="367702">/bin/Sitecore.Support.367702.dll</assembly>
          </assemblies>
        </processor>

        <!-- Processor patches defaultUrlOptions for current LinkProvider. The defaultUrlOptions.SiteResolving property is set in accordance with the Rendering.SiteResolving setting value. -->
        <processor type="Sitecore.Support.SiteResolvingPatcher, Sitecore.Support.367702" />
      </initialize>
    </pipelines>
    <sitecore.experienceeditor.speak.requests>
      <request name="ExperienceEditor.Language.ChangeLanguage" type="Sitecore.Support.ExperienceEditor.Speak.Ribbon.Requests.ChangeLanguage.ChangeLanguageRequest, Sitecore.Support.367702" patch:instead="*[@type='Sitecore.ExperienceEditor.Speak.Ribbon.Requests.ChangeLanguage.ChangeLanguageRequest, Sitecore.ExperienceEditor.Speak.Ribbon']"/>
    </sitecore.experienceeditor.speak.requests>
  </sitecore>
</configuration>